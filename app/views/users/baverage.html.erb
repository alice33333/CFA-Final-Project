<% provide(:title, 'Back of House Averages') %>
<h2>Back of House Averages</h2>
    
    <% i = 1 %>
    <% @users.each do |a| %>
     <% if a.role == 5 || a.role == 2 %>
       <div class="form-inline">
    <div class="form-group">
        <h17><%= i%> </h17>
        </div>
        <div class="form-group">
        <h17><%= "."%> </h17>
        </div>
        <div class="form-group">
        <h2><%= a.name.to_s %> </h2>
        </div>
        <div class="form-group">
        <h2><%= a.overall.to_s %> </h2>
        </div>
        <% i = i + 1 %>
    </div>
    <% end %>
    <% end %>